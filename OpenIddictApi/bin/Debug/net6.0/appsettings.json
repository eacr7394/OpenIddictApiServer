{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "OpenIddictApiServerContextKV": {
    "Server": "localhost",
    "Port": 3306,
    "Database": "open_iddict_api_server",
    "User": "#ApiServer.SignatureApiMySqlContextSettings.User#",
    "Password": "#ApiServer.SignatureApiMySqlContextSettings.Password#",
    "CertificateFile": "#ApiServer.SignatureApiMySqlContextSettings.CertificateFile#",
    "CertificatePassword": "#ApiServer.SignatureApiMySqlContextSettings.CertificatePassword#"
  }
}
